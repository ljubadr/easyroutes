{
    "name": "ljubadr/easyroutes",
    "type": "library",
    "description": "Web interface for laravel routes",
    "keywords": [
        "laravel",
        "web",
        "routes",
        "helper"
    ],
    "homepage": "https://github.com/ljubadr/easyroutes",
    "license": "MIT",
    "authors": [
        {
            "name": "Ljubomir Drakulovic",
            "email": "ljubadr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ljubadr\\EasyRoutes\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ljubadr\\EasyRoutes\\EasyRoutesServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Ljubadr\\EasyRoutes\\Providers\\EasyRoutesServiceProvider\""
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"Ljubadr\\EasyRoutes\\Providers\\EasyRoutesServiceProvider\" --tag=\"assets\""
        ]
    }
}
